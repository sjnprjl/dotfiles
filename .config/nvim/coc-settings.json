{
    "suggest.noselect": false, 
    "suggest.enablePreview": true, 
    "suggest.removeDuplicateItems": true, 
    "pyright.enable": true, 
    "codeLens.enable": true,
    "coc.preferences.jumpCommand": "drop",
    "coc.preferences.rootPatterns": [".git", ".vscode"], 
    "coc.preferences.formatOnSaveFiletypes": [
        "cpp",
        "bash",
        "c",
        "css",
        "javascript",
        "html",
        "php", 
        "java", 
        "python"
    ], 
    "diagnostic.checkCurrentLine": false,
    "tsserver.npm": "/usr/local/bin/npm",
    "javascript.referencesCodeLens.enable": false,
    "javascript.implementationsCodeLens.enable": false,
    "typescript.referencesCodeLens.enable": false,
    "typescript.implementationsCodeLens.enable": false, 
    "python.formatting.provider": "autopep8", 
    "python.formatting.autopep8Args": ["--aggressive",  "--aggressive"], 
    "python.linting.flake8Enabled": true, 
    "python.jediEnabled": true, 
    "python.analysis.diagnosticEnabled": true, 
    "python.autoComplete.extraPaths": []

}
